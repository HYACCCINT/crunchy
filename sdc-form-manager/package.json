{
  "name": "sdc-form-manager",
  "version": "1.0.0",
  "scripts": {
    "server": "node build/server.js",
    "client": "cd client && npm run start",
    "build": "npx tsc && cd client && npm run build",
    "iall": "npm i && cd client && npm i",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@cloudant/cloudant": "^4.3.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-graphql": "^0.11.0",
    "fs": "0.0.1-security",
    "graphql": "^15.3.0",
    "nodemon": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^3.4.4",
    "urql": "^1.10.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "typescript": "^4.0.3"
  }
}
